class Solution {
public:
    int findCenter(vector<vector<int>>& edges) {
        int n = edges.size();
        int ans = 0;
        if(edges[1][0]==edges[0][0]){
            return edges[0][0];
        }
        else if(edges[1][0]==edges[0][1]){
            return edges[0][1];
        }
        else if(edges[1][1]==edges[0][0]){
            return edges[0][0];
        }
        else{
            return edges[0][1];
        }
    }
};
