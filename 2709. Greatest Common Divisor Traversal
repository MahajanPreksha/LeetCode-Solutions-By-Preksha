class Solution {
public:
    bool canTraverseAllPairs(vector<int>& nums) {
        if(nums.size()==1){
            return true;
        }
        unordered_set<int> s(begin(nums), end(nums));
        if(s.size()==1){ 
            if(s.find(1)==s.end()){
                return 1;
            }
            return 0;
        }
        for(int i=0; i<nums.size(); i++){
            int f = 0;
            for(int j=0; j<nums.size(); j++){
                if(j!=i && __gcd(nums[i], nums[j])>1 && nums[i]!=nums[j]){
                    f = 1;
                    break;
                }
            }
            if(f==0){
                return false;
            }
        }
        return true;
    }
};
